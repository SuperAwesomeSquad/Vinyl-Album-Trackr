<%= render 'album' %>
<% if !current_user.album_ids.include? @album.id %>
<%= link_to 'Add This Album', albums_path(:discogs_id => @album.discogs_id), :method => 'POST' %>
<% end %>
<%= link_to "Edit Album", edit_album_path(@album) %>
<%= link_to "Delete Album",
	album_path(@album),
	:method => :delete,
	:confirm => "Are you sure you want to delete this album?" %>
